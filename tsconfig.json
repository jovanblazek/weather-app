{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": "src"
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.js", "src", "assets"],
  "exclude": ["node_modules"]
}
